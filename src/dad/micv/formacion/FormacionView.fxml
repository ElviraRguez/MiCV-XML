<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="view" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="formacionTable" editable="true" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="desdeFormColumn" prefWidth="75.0" text="Desde" />
                <TableColumn fx:id="hastaFormColumn" prefWidth="75.0" text="Hasta" />
                  <TableColumn fx:id="denominacioFormColumn" prefWidth="99.0" text="Denominación" />
                  <TableColumn fx:id="organizadorFormColumn" prefWidth="98.0" text="Organizador" />
              </columns>
            </TableView>
            <VBox spacing="5.0">
               <children>
                  <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onAgregarFormacionButtonAction" text="Añadir" />
                  <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onEliminarFormacionButtonAction" text="Eliminar" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </center>
</BorderPane>
